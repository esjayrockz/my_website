<div id="post_show_content" class="skinny_wrapper wrapper_padding">
  <header>
    <p class="date"><%=@project.created_at.strftime('%A, %B %d')%></p>
    <h1><%=@project.title%></h1>
    <hr>
  </header>
  <div id="content">

    <%=@project.description%>

  </div>

</div>

<%= @project.title %> <br> <br>
<%= @project.description %> <br> <br>
<%= @project.link %><br> <br>
<%= @project.time_period %>
<br>
<br>

<%= link_to "Edit", edit_project_path(@project) %>
<%= link_to "Delete", project_path(@project), method: :delete, data: {confirm: "Are you sure you want to delete this project?"} %>
