<h1 id="page_title">Projects</h1>
<div id="posts_wrapper" class="skinny_wrapper">
  <% @projects.each do |project| %>
<div class="post">
  <p class="date"><%= project.time_period %></p>
  <h2><a href="#"><%= link_to project.title, project %></a></h2>
  <hr>
</div>
  <% end %>


<div class="pagination">
  <span class="previous_page disabled">
    Previous
  </span>
  <em class="current">1</em>
  <a rel="next" href="#">2</a>
  <a href="#" class="next_page">Next</a>
</div>
